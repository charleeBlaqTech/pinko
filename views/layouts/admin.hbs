        const usetim = document.getElementById('usetim');
        usetim.innerHTML = today;



        var icon = "{{icon}}"
        var alerte = "{{alerte}}"
        var title = "{{ title}}"

        if (alerte) {
            swal({
                title: title,
                text: alerte,
                icon: icon,
                button: {
                    text: 'Continue',
                    value: true,
                    visible: true,
                    className: 'btn btn-primary',
                },
            });
        }
    </script>

